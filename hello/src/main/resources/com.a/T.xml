<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.a.dao.TDao">
    <select id="selectT" resultType="java.lang.Integer" >
        select count(*) from t where name=#{0}
    </select>
    <select id="getT" resultType="com.a.domain.T" >
        select * from user where name=#{0}
    </select>
</mapper>
